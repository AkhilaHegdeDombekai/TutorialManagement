@using System.Activities.Expressions
@using System.Collections.Specialized
@using System.Web.Mvc.Html
@using System.Web.Services.Description
<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="">
    <meta name="author" content="">

    <title>@ViewBag.Title -Tutorial Management</title>

    <!-- Bootstrap Core CSS -->
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" type="text/css">

    <!-- Fonts -->
    <link href="~/font-awesome/css/font-awesome.min.css" rel="stylesheet" type="text/css">
    <link href="~/Content/animate.css" rel="stylesheet" />
    <!-- Squad theme CSS -->
    <link href="~/Content/style.css" rel="stylesheet">
    <link href="~/color/default.css" rel="stylesheet">

</head>

<body id="page-top" data-spy="scroll" data-target=".navbar-custom">
    <!-- Preloader -->
    <div id="preloader">
        <div id="load"></div>
    </div>

    <nav class="navbar navbar-custom navbar-fixed-top" role="navigation">
        <div class="container">
            <div class="navbar-header page-scroll">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-main-collapse">
                    <i class="fa fa-bars"></i>
                </button>
                <a class="navbar-brand" href="#">
                    <h1>TUTORIAL MANAGEMENT</h1>
                </a>
            </div>

            <!-- Collect the nav links, forms, and other content for toggling -->
            <div class="collapse navbar-collapse navbar-right navbar-main-collapse">
                <ul class="nav navbar-nav">
                    <li class="active"><a href="#intro">Home</a></li>
                    <li>@Html.ActionLink("About", "UploadDocument", "Document")</li>
                    <li><a href="#service">Service</a></li>
                    <li><a href="#contact">Contact</a></li>
                    <li class="dropdown">
                        <a href="#" class="dropdown-toggle" data-toggle="dropdown">MANAGE DOCUMENT<b class="caret"></b></a>
                        <ul class="dropdown-menu">
                            <li><a href="#">Upload A Document</a></li>
                            <li><a href="#">Download A Document</a></li>
                            <li><a href="#">Search For A Document</a></li>
                            <li><a href="#">Help</a></li>
                        </ul>
                    </li>
                    <li class="dropdown">
                        <a href="#" class="dropdown-toggle" data-toggle="dropdown">ADMIN PRIVILEGE<b class="caret"></b></a>
                        <ul class="dropdown-menu">
                            <li><a href="#">Manage User</a></li>
                            <li><a href="#">Manage Uploaded Document</a></li>
                            <li><a href="#">Add Branch</a></li>
                            <li><a href="#">Add BitVector32.Section</a></li>
                        </ul>
                    </li>
                </ul>
            </div>
            <!-- /.navbar-collapse -->
        </div>
        <!-- /.container -->
    </nav>

    <!-- BitVector32.Section: intro -->
    @RenderBody()
    <!-- /Section: intro -->
    
    

    <footer>
        <div class="container">
            <div class="row">
                <div class="col-md-12 col-lg-12">
                    
                    <p>&copy;Tutorial Management system-@DateTime.UtcNow.Year<br/> S I G G F G Women's College. All rights reserved.</p>
                    <div class="credits">
                       Final year BSC students-2015-16
                    </div>
                </div>
            </div>
        </div>
    </footer>

    <!-- Core JavaScript Files -->
    <script src="~/Scripts/Common/jquery.min.js"></script>
    <script src="~/Scripts/Common/bootstrap.min.js"></script>
    <script src="~/Scripts/Common/jquery.easing.min.js"></script>
    <script src="~/Scripts/Common/jquery.scrollTo.js"></script>
    <script src="~/Scripts/Common/wow.min.js"></script>
    <!-- Custom Theme JavaScript -->
    <script src="~/Scripts/Common/custom.js"></script>
    <script src="~/Scripts/Common/contactform.js"></script>

</body>

</html>


